{
  "type": "object",
  "properties": {
    "isMyview": {
      "type": "identifier",
      "selector": ".subtle",
      "control": "exists",
      "selectContains": "Assigned to Me (Unresolved)"
    },
    "viewissueslink": {
      "type": "action",
      "selector": "a[href*='view_all_bug_page.php']",
      "control": "action.link"
    }
  },
  "connect": [
    {
      "match": [
        "isMyview"
      ],
      "action": "viewissueslink"
    }
  ]
}
