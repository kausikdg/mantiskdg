{
  "type": "object",
  "comment": "issueslist",
  "properties": {
    "issueslist": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selector": "td",
            "selectIndex": 3,
            "control": "property.innerText"
          },
          "summary": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selector": "tc",
            "selectIndex": 10,
            "control": "property.innerText"
          },
          "status": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selector": "td",
            "selectIndex": 8,
            "control": {
              "fn": "function(param){var i=param.indexOf('(');if(i>0){return param.substring(0,i-1)}else{return param}}",
              "control": "property.innerText"
            }
          },
          "severity": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selector": "td",
            "selectIndex": 7,
            "control": "property.innerText"
          },
          "assignedto": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selector": "td",
            "selectIndex": 8,
            "control": {
              "fn": "function(param){var i=param.indexOf(' ');var len=param.length;if(i>0){return param.substring(i+2,len-1)}else{return 'not assigned'}}",
              "control": "property.innerText"
            }
          },
          "updated": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selector": "td",
            "selectIndex": 9,
            "control": "property.innerText"
          }
        }
      },
      "update": {
        "type": "none"
      },
      "selectorAll": "#buglist tbody tr[bgcolor]"
    },
    "isViewissues": {
      "type": "identifier",
      "selector": ".floatleft",
      "control": "exists",
      "selectContains": "Viewing Issues"
    }
  },
  "connect": [
    {
      "match": [
        "isViewissues"
      ],
      "screens": [
        "viewissues"
      ]
    }
  ]
}
